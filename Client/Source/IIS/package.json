{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for Angular",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8081",
    "test": "karma start",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build-aot": "rimraf dist && webpack --config config/webpack.prod.aot.js --progress --profile --bail",
    "build-dev": "rimraf dist && webpack --config config/webpack.dev.js  --watch --progress --profile --bail",
    "lint": "tslint \"src/app/**/*.ts\"",
    "clean": "rimraf node_modules"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "4.1.3",
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/compiler-cli": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/router": "~4.0.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "mydatepicker": "^1.7.16",
    "ng2-bootstrap": "^1.3.3",
    "ng2-currency-mask": "^4.0.2",
    "ng2-date-picker": "^0.6.2",
    "primeng": "^4.0.0",
    "rxjs": "5.0.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@ngtools/webpack": "1.2.10",
    "@types/jasmine": "2.5.36",
    "@types/jquery": "^2.0.40",
    "@types/node": "^6.0.45",
    "angular-2-local-storage": "1.0.1",
    "angular2-router-loader": "^0.2.2",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.4",
    "codelyzer": "~2.0.0-beta.4",
    "css-loader": "^0.26.1",
    "electron": "^1.6.10",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-electron": "^5.2.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "moment": "^2.18.1",
    "ng2-bootstrap-modal": "1.0.1",
    "node-sass": "^4.5.1",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "tslint": "~4.4.0",
    "typescript": "~2.2.1",
    "wallaby-webpack": "0.0.32",
    "webpack": "^2.2.1",
    "webpack-dev-server": "2.4.1",
    "webpack-merge": "^3.0.0"
  }
